    function change_metric_system(){
        if(metric_system == 'mm') {
            metric_system = 'ft';
            $("#metricSystem")[0].innerHTML='<a class="tooltip" onclick = "change_metric_system()"><span style="font-weight: bold;font-size: 22px;"><center>m</center></span><span class="tooltiptext">Toggle Dimension Unit</span></a>';
              
        }
        else if(metric_system == 'ft'){
            metric_system = 'mm';                
            $("#metricSystem")[0].innerHTML='<a class="tooltip" onclick = "change_metric_system()"><span style="font-weight: bold;font-size: 22px;"><center>ft</center></span><span class="tooltiptext">Toggle Dimension Unit</span></a>';
            
        }   
    }

    function close_panel(){
        // $('.panel_content').hide();
        // $("#info").animate({ width: 'hide' });
        // $('#sidebar-menu li').removeClass('active').removeClass('active-sm');
        elem_panel = "";

        $('.panel_content').show();

        $("#panel_fixed_header li").each(function(){
            $(this).removeClass("active_panel_header_li");
        })
        $(".Project").addClass("active_panel_header_li");                                       
       

        $("#info").animate({ width: 'show' });
        $("#Project").slideDown();
        $("#Room").hide();
        $("#Window").hide();
        $("#Door").hide();
        $("#modifyCompartment").hide();
        $("#rightClickMenu").hide();


        
    }
</script>

